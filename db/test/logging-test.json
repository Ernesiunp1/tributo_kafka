{
  "version": 1,
    "loggers": {
      "root": {
        "handlers": [
          "console"
        ],
        "level": "WARNING"
      },
      "anfler.db": {
        "propagate": false,
        "handlers": [
          "console"
        ],
        "level": "INFO"
      },
      "sqlalchemy.engine": {
        "propagate": false,
        "handlers": [
          "console"
        ],
        "level": "INFO"
      }
    },
    "formatters": {
      "basic": {
        "class": "logging.Formatter",
        "datefmt": "%Y-%m-%d %H:%M:%S",
        "format": "%(asctime)s.%(msecs)03d %(levelname)s %(threadName)s %(module)s - %(funcName)s: %(message)s"
      },
      "raw": {
        "class": "logging.Formatter",
        "format": "%(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "formatter": "basic"
      }
    }
}